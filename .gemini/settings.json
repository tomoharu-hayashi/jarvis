{
  "model": {
    "name": "gemini-3-pro-preview"
  },
  "mcpServers": {
    "calculator": {
      "command": "uvx",
      "args": ["mcp-server-calculator"]
    },
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone=Asia/Tokyo"]
    },
    "desktop": {
      "command": "mcp-desktop",
      "trust": true
    },
    "context7": {
		"command": "bunx",
		"args": ["-y", "@upstash/context7-mcp@latest"],
		"enabled": true
	  },
    "skills": {
      "command": "mcp-skills",
      "trust": true
    }
  }
}
